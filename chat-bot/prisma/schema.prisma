generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
}

model cart_items {
  id       String  @id @db.VarChar(255)
  price    Float?
  quantity Float?
  cart_id  String? @db.VarChar(255)
  menu_id  String? @db.VarChar(255)
  menus    menus?  @relation(fields: [menu_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fkcgxdkukntyc1bqo7gkml96657")
  carts    carts?  @relation(fields: [cart_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fkpcttvuq4mxppo8sxggjtn5i2c")
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model carts {
  id         String       @id @db.VarChar(255)
  created_at DateTime?    @db.Timestamp(6)
  status     String?      @db.VarChar(255)
  user_id    String?      @unique(map: "uk64t7ox312pqal3p7fg9o503c2") @db.VarChar(255)
  cart_items cart_items[]
  users      users?       @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fkb5o626f86h46m4s7ms6ginnop")
}

model discount {
  id            String       @id @db.VarChar(255)
  discount      Float
  restaurant_id String?      @db.VarChar(255)
  restaurants   restaurants? @relation(fields: [restaurant_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fkp0cftifugb9p12amyx41c0bgn")
}

model menus {
  id          String       @id @db.VarChar(255)
  description String?      @db.VarChar(255)
  image       String?      @db.VarChar(255)
  is_veg      Boolean
  name        String?      @db.VarChar(255)
  price       String?      @db.VarChar(255)
  rating      Float
  votes       BigInt
  cart_items  cart_items[]
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model payments {
  id         String    @id @db.VarChar(255)
  amount     Float?
  created_at DateTime? @db.Timestamp(6)
  menu_id    String?   @db.VarChar(255)
  order_id   String?   @db.VarChar(255)
  status     String?   @db.VarChar(255)
  user_id    String?   @db.VarChar(255)
  users      users?    @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fkj94hgy9v5fw1munb90tar2eje")
}

model restaurants {
  id       String     @id @db.VarChar(255)
  cost     String?    @db.VarChar(255)
  image    String?    @db.VarChar(255)
  location String?    @db.VarChar(255)
  name     String?    @db.VarChar(255)
  rating   Float
  discount discount[]
}

model users {
  id       String     @id @db.VarChar(255)
  email    String?    @unique(map: "uk6dotkott2kjsp8vw4d0m25fb7") @db.VarChar(255)
  name     String?    @unique(map: "uk3g1j96g94xpk3lpxl2qbl985x") @db.VarChar(255)
  password String?    @db.VarChar(255)
  carts    carts?
  payments payments[]
}
